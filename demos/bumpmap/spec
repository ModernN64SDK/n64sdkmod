#include "bumpmap.h"

/*
 * ROM spec file
 */
beginseg
	name "code"
	flags BOOT OBJECT
	entry boot
	address 0x80200000
	stack bootStack + STACKSIZE
	include "codesegment.o"
	include "$(ROOT)/usr/lib/PR/rspboot.o"
	include "$(ROOT)/usr/lib/PR/gspF3DEX2.xbus.o"
	include "$(ROOT)/usr/lib/PR/gspF3DEX2.fifo.o"
endseg

beginseg
	name "cfb"
	flags OBJECT
	address 0x80100000
	include "cfb.o"
endseg

beginseg
	name "zbuf"
	flags OBJECT
	address 0x80080000
	include "zbuf.o"
endseg

beginseg
	name "static"
	flags OBJECT
	number STATIC_SEGMENT
	include "static.o"
endseg

beginseg
	name "rsp_cfb"
	flags OBJECT
	number CFB_SEGMENT
	include "rsp_cfb.o"
endseg

beginwave
	name "bumpmap"
	include "code"
	include "static"
	include "cfb"
	include "rsp_cfb"
	include "zbuf"
endwave
