#include "system.h"

beginseg
	name "code"
	flags BOOT OBJECT
	entry boot
	stack bootStack + STACKSIZE
	include "codesegment.o"
	include	"/usr/lib/n64/PR/rspboot.o"
	include	"/usr/lib/n64/PR/gspZSort.fifo.o"
endseg

beginseg
        name "system_cfb"
        flags OBJECT
	address CFB_ADDRESS
        include "system_cfb.o"
endseg

beginseg
        name "system_rspbuf"
        flags OBJECT
#ifdef	RSPBUF_ADDRESS
	address	RSPBUF_ADDRESS
#else
	after system_cfb
#endif
        include "system_rspbuf.o"
endseg

beginwave
	name    "zonetri"
	include	"code"
	include	"system_cfb"
	include	"system_rspbuf"
endwave

/*======== End of spec ========*/
