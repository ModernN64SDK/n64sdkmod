
#include "Task.h"
#include "RcpRdp.h"
#include "FrameBuffer.h"

/*
 * ROM spec file
 */
beginseg
	name "code"
	flags BOOT OBJECT
	entry boot
	stack bootStack + STACKSIZE
	include "codesegment.o"
	include "/usr/lib/n64/PR/rspboot.o"
	include "/usr/lib/n64/PR/gspS2DEX2.fifo.o"
endseg

beginseg
	name "cfb"
	flags OBJECT
	address 0x80300000
	include "cfb.o"
endseg

beginseg
	name "static"
	flags OBJECT
        after "code"
	include "static.o"
endseg

beginseg
	name "rsp_cfb"
	flags OBJECT
	number CFB_SEGMENT
	include "rsp_cfb.o"
endseg

beginseg
	name "dk"
	flags OBJECT
	after "static"
	include "dk.o"
endseg

beginseg
	name "dk7"
	flags OBJECT
	after "dk"
	include "dk7.o"
endseg

beginseg
	name "road"
	flags OBJECT
	after "dk7"
	include "road.o"
endseg

beginseg
	name "l2_tv"
	flags OBJECT
	after "road"
	include "l2_tv.o"
endseg

beginwave
	name "tile_rect"
	include "code"
	include "static"
	include "cfb"
	include "rsp_cfb"

        include "dk"
        include "dk7"
        include "road"
        include "l2_tv"
endwave
